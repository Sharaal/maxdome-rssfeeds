<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>{{ channel.title }}</title>
    <description></description>
    <link>{{ channel.link }}</link>
    {%- for item in items %}
    <item>
      <guid>{{ item.guid }}</guid>
      <title>{{ item.title }}</title>
      <description>{{ item.description }}</description>
      <link>{{ item.link }}</link>
    </item>
    {%- endfor %}
  </channel>
</rss>
